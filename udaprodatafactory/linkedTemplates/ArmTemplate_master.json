{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "udaprodatafactory"
		},
		"ls_AzureSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_AzureSqlDatabase'"
		},
		"ls_SynapseAnalytics_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_SynapseAnalytics'"
		},
		"ls_azuredatalakegen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_azuredatalakegen2'"
		},
		"ls_SynapseAnalytics_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "udaprosynapseworkspace-ondemand.sql.azuresynapse.net"
		},
		"ls_SynapseAnalytics_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "Udacity"
		},
		"ls_SynapseAnalytics_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"ls_azuredatalakegen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://udaprostorageaccount.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "udaprodatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_AzureSqlDatabase_connectionString": {
						"value": "[parameters('ls_AzureSqlDatabase_connectionString')]"
					},
					"ls_SynapseAnalytics_password": {
						"value": "[parameters('ls_SynapseAnalytics_password')]"
					},
					"ls_azuredatalakegen2_accountKey": {
						"value": "[parameters('ls_azuredatalakegen2_accountKey')]"
					},
					"ls_SynapseAnalytics_properties_typeProperties_server": {
						"value": "[parameters('ls_SynapseAnalytics_properties_typeProperties_server')]"
					},
					"ls_SynapseAnalytics_properties_typeProperties_database": {
						"value": "[parameters('ls_SynapseAnalytics_properties_typeProperties_database')]"
					},
					"ls_SynapseAnalytics_properties_typeProperties_userName": {
						"value": "[parameters('ls_SynapseAnalytics_properties_typeProperties_userName')]"
					},
					"ls_azuredatalakegen2_properties_typeProperties_url": {
						"value": "[parameters('ls_azuredatalakegen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "udaprodatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'udaprodatafactory_ArmTemplate_0')]"
			]
		}
	]
}